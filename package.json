{
	"name": "gapline",
	"displayName": "Line Gapper",
	"description": "Extensión que permite insertar una línea en blanco cada N líneas de código.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [ "Other" ],
	"activationEvents": [ "onCommand:extension.gapline" ],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.gapline",
				"title": "Line Gapper"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
